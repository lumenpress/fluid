{
    "name": "lumenpress/orm",
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "^5.4"
    },
    "require-dev": {
        "kint-php/kint": "^2.1",
        "phpunit/phpunit": "^6.3",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
